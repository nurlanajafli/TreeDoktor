{
    "name": "symplify/package-builder",
    "description": "Dependency Injection, Console and Kernel toolkit for Symplify packages.",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.2",
        "symfony/config": "^4.4|^5.3",
        "symfony/console": "^4.4|^5.3",
        "symfony/dependency-injection": "^5.3",
        "symfony/finder": "^4.4|^5.3",
        "symfony/http-kernel": "^4.4|^5.3",
        "symplify/symplify-kernel": "^9.3.26",
        "symplify/easy-testing": "^9.3.26",
        "nette/neon": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PackageBuilder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PackageBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "9.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
